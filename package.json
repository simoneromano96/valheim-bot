{
  "name": "valheim-bot",
  "version": "0.0.0",
  "description": "A simple valheim bot test",
  "main": "./src/index.js",
  "scripts": {
    "dev": "ts-node-dev --experimental-specifier-resolution=node --no-notify ./src/index.ts",
    "start:ts": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "start": "node --experimental-specifier-resolution=node ./dist/index.js",
    "build": "tsc",
    "lint": "eslint './**/*.ts'",
    "lint-and-fix": "eslint './**/*.ts' --fix",
    "check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simoneromano96/valheim-bot.git"
  },
  "author": "Simone Romano [simoneromano@tutanota.de], Ayrton []",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simoneromano96/valheim-bot/issues"
  },
  "homepage": "https://github.com/simoneromano96/valheim-bot#readme",
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.2.88",
    "@swc/helpers": "^0.2.13",
    "@types/dockerode": "^3.2.7",
    "@types/level": "^6.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.9.2",
    "@types/pino": "^6.3.11",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@nexusmods/nexus-api": "^1.1.3",
    "@sinclair/typebox": "^0.20.5",
    "bullmq": "^1.47.1",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dockerode": "^3.3.1",
    "fastify": "^3.21.3",
    "fastify-auth": "^1.1.0",
    "fastify-basic-auth": "^2.1.0",
    "fastify-static": "^4.2.3",
    "fastify-swagger": "^4.12.0",
    "got": "^11.8.2",
    "level": "^7.0.0",
    "mime-types": "^2.1.32",
    "p-map": "^5.1.0",
    "pino": "^6.13.2",
    "pino-pretty": "^7.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint './**/*.ts' --fix",
      "git add"
    ]
  }
}
